services:
    typesense:
        image: 'typesense/typesense:30.1'
        restart: on-failure
        ports:
            - '8108:8108'
        volumes:
            - './typesense-data:/data'
        command: '--data-dir /data --api-key=xyz --enable-cors'
volumes:
    sail-typesense:
        driver: local
